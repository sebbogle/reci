      "Id": "c3d4e5f6-a7b8-9012-cdef-123456789012",
      "Name": "Homemade Lasagna",
      "Description": "A rich and hearty Italian classic with layers of pasta, meat sauce, and cheese",
      "GroupId": "33333333-3333-3333-3333-333333333333",
      "Ingredients": [
        {
          "Name": "Lasagna noodles",
          "QuantityAmount": 12,
          "QuantityUnit": "sheets",
          "GroupId": "aaaaaaaa-aaaa-aaaa-aaaa-aaaaaaaaaaaa"
        },
        {
          "Name": "Ground beef",
          "QuantityAmount": 1,
          "QuantityUnit": "lb",
          "GroupId": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb"
        },
        {
          "Name": "Italian sausage",
          "QuantityAmount": 0.5,
          "QuantityUnit": "lb",
          "GroupId": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb"
        },
        {
          "Name": "Onion, diced",
          "QuantityAmount": 1,
          "QuantityUnit": "whole",
          "GroupId": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb"
        },
        {
          "Name": "Garlic cloves, minced",
          "QuantityAmount": 4,
          "QuantityUnit": "cloves",
          "GroupId": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb"
        },
        {
          "Name": "Crushed tomatoes",
          "QuantityAmount": 28,
          "QuantityUnit": "oz",
          "GroupId": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb"
        },
        {
          "Name": "Tomato paste",
          "QuantityAmount": 6,
          "QuantityUnit": "oz",
          "GroupId": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb"
        },
        {
          "Name": "Dried basil",
          "QuantityAmount": 2,
          "QuantityUnit": "tsp",
          "GroupId": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb"
        },
        {
          "Name": "Dried oregano",
          "QuantityAmount": 1,
          "QuantityUnit": "tsp",
          "GroupId": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb"
        },
        {
          "Name": "Ricotta cheese",
          "QuantityAmount": 15,
          "QuantityUnit": "oz",
          "GroupId": "cccccccc-cccc-cccc-cccc-cccccccccccc"
        },
        {
          "Name": "Egg",
          "QuantityAmount": 1,
          "QuantityUnit": "whole",
          "GroupId": "cccccccc-cccc-cccc-cccc-cccccccccccc"
        },
        {
          "Name": "Fresh parsley, chopped",
          "QuantityAmount": 0.25,
          "QuantityUnit": "cup",
          "GroupId": "cccccccc-cccc-cccc-cccc-cccccccccccc"
        },
        {
          "Name": "Mozzarella cheese, shredded",
          "QuantityAmount": 4,
          "QuantityUnit": "cups",
          "GroupId": "cccccccc-cccc-cccc-cccc-cccccccccccc"
        },
        {
          "Name": "Parmesan cheese, grated",
          "QuantityAmount": 0.75,
          "QuantityUnit": "cup",
          "GroupId": "cccccccc-cccc-cccc-cccc-cccccccccccc"
        }
      ],
      "Instructions": [
        {
          "Text": "Cook lasagna noodles according to package directions. Drain and set aside",
          "Group": "Preparation",
          "GroupId": "dddddddd-dddd-dddd-dddd-dddddddddddd"
        },
        {
          "Text": "In a large skillet, cook ground beef, Italian sausage, and onion over medium heat until meat is browned",
          "Group": "Meat Sauce",
          "GroupId": "eeeeeeee-eeee-eeee-eeee-eeeeeeeeeeee"
        },
        {
          "Text": "Add garlic and cook for 1 minute until fragrant",
          "Group": "Meat Sauce",
          "GroupId": "eeeeeeee-eeee-eeee-eeee-eeeeeeeeeeee"
        },
        {
          "Text": "Stir in crushed tomatoes, tomato paste, basil, and oregano. Simmer for 30 minutes",
          "Group": "Meat Sauce",
          "GroupId": "eeeeeeee-eeee-eeee-eeee-eeeeeeeeeeee"
        },
        {
          "Text": "In a bowl, mix ricotta cheese, egg, and parsley until well combined",
          "Group": "Cheese Mixture",
          "GroupId": "ffffffff-ffff-ffff-ffff-ffffffffffff"
        },
        {
          "Text": "Preheat oven to 375�F (190�C)",
          "Group": "Assembly",
          "GroupId": "00000000-0000-0000-0000-000000000000"
        },
        {
          "Text": "Spread 1 cup of meat sauce in the bottom of a 9x13 inch baking dish",
          "Group": "Assembly",
          "GroupId": "00000000-0000-0000-0000-000000000000"
        },
        {
          "Text": "Layer 4 noodles over the sauce",
          "Group": "Assembly",
          "GroupId": "00000000-0000-0000-0000-000000000000"
        },
        {
          "Text": "Spread 1/3 of the ricotta mixture over noodles",
          "Group": "Assembly",
          "GroupId": "00000000-0000-0000-0000-000000000000"
        },
        {
          "Text": "Sprinkle with 1 cup mozzarella and 1/4 cup Parmesan",
          "Group": "Assembly",
          "GroupId": "00000000-0000-0000-0000-000000000000"
        },
        {
          "Text": "Repeat layers twice more",
          "Group": "Assembly",
          "GroupId": "00000000-0000-0000-0000-000000000000"
        },
        {
          "Text": "Top with remaining meat sauce and cheeses",
          "Group": "Assembly",
          "GroupId": "00000000-0000-0000-0000-000000000000"
        },
        {
          "Text": "Cover with foil and bake for 25 minutes",
          "Group": "Baking",
          "GroupId": "aaaabbbb-cccc-dddd-eeee-ffff00001111"
        },
        {
          "Text": "Remove foil and bake for an additional 25 minutes until cheese is golden and bubbly",
          "Group": "Baking",
          "GroupId": "aaaabbbb-cccc-dddd-eeee-ffff00001111"
        },
        {
          "Text": "Let stand for 15 minutes before serving",
          "Group": "Baking",
          "GroupId": "aaaabbbb-cccc-dddd-eeee-ffff00001111"
        }
      ],
      "NutritionInfo": {
        "Calories": 520,
        "Fat": 28.0,
        "Carbohydrates": 35.0,
        "Protein": 32.0
      },
      "Tags": [
        "italian",
        "pasta",
        "main-course",
        "comfort-food",
        "family-friendly"
      ],
      "FurtherNotes": "Serves 8-10. Can be prepared ahead and refrigerated overnight before baking. Freezes well for up to 3 months."
    },
